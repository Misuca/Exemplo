            @Html.DisplayFor(modelItem => item.HoraFim)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Utilizador.Nome)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { idUtilizador=item.Id_Utilizador , idViatura = item.Id_Viatura }) |
            @Html.ActionLink("Details", "Details", new { idUtilizador=item.Id_Utilizador , idViatura = item.Id_Viatura }) |
