@model IEnumerable<Exemplo.Models.Utilizaçao_Temporaria>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    <a class="btn btn-primary" href="@Url.Action("Create")"><span class="glyphicon glyphicon-plus"></span></a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Viatura.Matricula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataInicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataFim)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HoraInicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HoraFim)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Utilizador.Nome)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Viatura.Matricula)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DataInicio)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DataFim)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.HoraInicio)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.HoraFim)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Utilizador.Nome)
    </td>
    <td>
        <a class="btn btn-primary" href="@Url.Action("Edit", new { id_UT = item.Id_UtilizacaoTemporaria})"><span class="glyphicon glyphicon-pencil"></span></a>
        <a class="btn btn-primary" href="@Url.Action("Details", new { id_UT = item.Id_UtilizacaoTemporaria})"><span class="glyphicon glyphicon-search"></span></a>
        <a class="btn btn-primary" href="@Url.Action("Delete", new {id_UT = item.Id_UtilizacaoTemporaria})"><span class="glyphicon glyphicon-trash"></span></a>
    </td>
</tr>
}

</table>
